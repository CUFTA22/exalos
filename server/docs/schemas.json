{
  "plannerData": {
    "type": "object"
  },

  "plannerBody": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "description": "User email, eg. nikolajovanovic5556@gmail.com",
        "required": true
      }
    }
  },

  "plannerCellBody": {
    "type": "object",
    "properties": {
      "cell_ids": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "description": "Array of cell ID's to update, usually only 1 id",
        "required": true
      },
      "text": {
        "type": "string",
        "description": "Text update for cell",
        "required": false
      },
      "type_id": {
        "type": "string",
        "description": "Type update for cell",
        "required": false
      },
      "meet_url": {
        "type": "string",
        "description": "Meet URL update for cell",
        "required": false
      }
    }
  }
}
