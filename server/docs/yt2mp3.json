{
  "/api/yt2mp3": {
    "get": {
      "tags": ["YT2MP3 Documentation"],
      "summary": "Download song from YouTube.",
      "operationId": "downloadSong",

      "parameters": [
        {
          "name": "url",
          "in": "path",
          "description": "YouTube video URL",
          "required": true,
          "type": "string"
        }
      ],

      "responses": {
        "200": {
          "description": "Song file"
        },
        "400": {
          "description": "No URL provided"
        }
      }
    }
  }
}
